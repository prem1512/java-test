pipeline {
    agent any
stages{
    stage("checkout")
    {
    steps {
    script{
        checkout([
        $class: 'GitSCM',
        branches: [[name: "main" ]], 
        userRemoteConfigs: [[
            url: "https://github.com/prem1512/java-test.git",
            credentialsId: "cred-git"
        ]]
    ])
    }
    }
}

    }
}